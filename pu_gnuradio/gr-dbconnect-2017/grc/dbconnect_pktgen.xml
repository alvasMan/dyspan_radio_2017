<?xml version="1.0"?>
<block>
  <name>pktgen</name>
  <key>dbconnect_pktgen</key>
  <category>dbconnect</category>
  <import>import dbconnect</import>
  <import>import pmt</import>
  <make>dbconnect.pktgen($interval, $pktsize, $quit, $debug, $rand_scen, $server, $port, $tmin, $tmax, $tconst, $mean1, $mean2, $mean3, $swtime, $psc, $seed, $gain_min, $gain_max, $gain_period, $gain_incr)</make>
	<param>
		<name>Period (ms)</name>
		<key>interval</key>
		<value>1000</value>
		<type>real</type>
	</param>
	<param>
		<name>Packet size(bytes)</name>
		<key>pktsize</key>
		<value>1500</value>
		<type>int</type>
	</param>
    <param>
      <name>Server IP</name>
      <key>server</key>
      <value>127.0.0.1</value>
      <type>string</type>
    </param>
    <param>
      <name>Port</name>
      <key>port</key>
      <value>5002</value>
      <type>int</type>
    </param>
    <param>
      <name>Delay-1</name>
      <key>tmin</key>
      <value>5</value>
      <type>int</type>
    </param>
    <param>
      <name>Delay-2</name>
      <key>tmax</key>
      <value>100</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean1</name>
      <key>mean1</key>
      <value>10</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean2</name>
      <key>mean2</key>
      <value>100</value>
      <type>int</type>
    </param>
    <param>
      <name>Mean3</name>
      <key>mean3</key>
      <value>50</value>
      <type>int</type>
    </param>
    <param>
      <name>Psuccess</name>
      <key>psc</key>
      <value>0.05</value>
      <type>float</type>
    </param>
    <param>
      <name>Seed</name>
      <key>seed</key>
      <value>6643</value>
      <type>int</type>
    </param>
    <param>
      <name>Tconst</name>
      <key>tconst</key>
      <value>0</value>
      <type>int</type>
    </param>
    <param>
      <name>Swtime</name>
      <key>swtime</key>
      <value>60000</value>
      <type>int</type>
    </param>
    <param>
      <name>Gain_min</name>
      <key>gain_min</key>
      <value>10</value>
      <type>int</type>
    </param>
    <param>
      <name>Gain_max</name>
      <key>gain_max</key>
      <value>30</value>
      <type>int</type>
    </param>
    <param>
      <name>Gain_per</name>
      <key>gain_period</key>
      <value>60000</value>
      <type>int</type>
    </param>
    <param>
      <name>Gain_incr</name>
      <key>gain_incr</key>
      <value>5</value>
      <type>int</type>
    </param>
	<param>
		<name>Quit</name>
		<key>quit</key>
		<value>True</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<param>
		<name>Debug</name>
		<key>debug</key>
		<value>False</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<param>
		<name>Randomize Scenarios</name>
		<key>rand_scen</key>
		<value>True</value>
		<type>bool</type>
		<option>
			<name>Enable</name>
			<key>True</key>
		</option>
		<option>
			<name>Disable</name>
			<key>False</key>
		</option>
	</param>
	<source>
		<name>out0</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>out1</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>out2</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>out3</name>
		<type>message</type>
		<optional>1</optional>
	</source>
	<source>
		<name>cmd</name>
		<type>message</type>
		<optional>1</optional>
	</source>
</block>
